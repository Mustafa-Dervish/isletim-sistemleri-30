eclipse.preferences.version=1    // Eclipse tercihlerinin sürümünü belirten satır 
org.eclipse.jdt.core.compiler.codegen.inlineJsrBytecode=enabled     // Derleyicinin JSR komutlarını yerine ilgili alt program kod dizilerini içeren kod dizileriyle değiştirmesini sağlar. Yazılan program sanal makinede daha hızlı çalışır..
org.eclipse.jdt.core.compiler.codegen.targetPlatform=17      // Derleyicisinin kod üretirken hedeflediği Java platformunu belirleyen kod satırı. Hedef platform Java 17.
org.eclipse.jdt.core.compiler.codegen.unusedLocal=preserve    // Derleyiciye kullanılmayan yerel değişkenleri koddan çıkarmaması gerektiğini belirtir. Hata ayıklama sırasında değişkenlerin değerlerini incelemeyi kolaylaştırır.
org.eclipse.jdt.core.compiler.compliance=17      // Derleyiciye Java17 ile uyumlu çalışmasını belirtir
org.eclipse.jdt.core.compiler.debug.lineNumber=generate     // Derleyicisinin hata ayıklama bilgileri üretirken kaynak kod satır numaralarını eklemesini sağlar. Varsayılan olarak generate olarak geçer. Generate olması kod boyutunu artırabilir
org.eclipse.jdt.core.compiler.debug.localVariable=generate    // Derleyicinin yerel değişkenler için hata ayıklama bilgisi üretmesini ve yerel değişkenlerin adlarını ve değerlerini göstermesini sağlar.
org.eclipse.jdt.core.compiler.debug.sourceFile=generate    // Derleyicisinin hata ayıklama bilgileri üretirken kaynak dosya adlarını eklemesini belirtir. 'generate' olarak seçilmesi dosya boyutunu büyütür.
org.eclipse.jdt.core.compiler.problem.assertIdentifier=error    // Derleyicisinin 'assert' anahtar sözcüğünün yanlış kullanımında nasıl işlenmesi gerektiğini tanımlar. Kod yanlış kullanılmış 'assert' anahtar kelimesini hata olarak algılar
org.eclipse.jdt.core.compiler.problem.enablePreviewFeatures=disabled    // Derleyicinin önizleme özelliklerinin kullanımını kontrol eden kod satırı. Burda 'disabled' anahtar kelimesi ile önizlemeyi devre dışı bıraktık
org.eclipse.jdt.core.compiler.problem.enumIdentifier=error    // Derleyicinin enum tanımlayıcıları ile ilgili bir hata ayarını temsil eder. Enum tanımlayıcıları için hata uyarısı verir.
org.eclipse.jdt.core.compiler.problem.reportPreviewFeatures=warning    // Derleyicinin önizleme özelliklerini (preview features) raporlama şeklini kontrol eder. 'warning' kelimesi ile uyarı mesajı verir. 
org.eclipse.jdt.core.compiler.release=enabled    // Derleyicinin hedef Java sürümünün özelliklerini ve API'lerini kullanmasına izin veren kod satırı. 'enabled' kelimesi ile buna izin verdik.
org.eclipse.jdt.core.compiler.source=17    // Derleyicide tanımlanan dil ve kabul edilecek özellikleri tanımlayan kod satırı Java SE 17'de tanıtılan dil özelliklerini ve API'leri kabul edecektir.
